page{
  width: 100%;
  height: 100%;
  background: rgb(250, 250, 250);
}
@import '../common/commonpage/loding_remax/loding_remax.wxss';
.topnav{
  width: 100%;
  height: 80rpx;
  text-align: center;
  position: relative;
}
.topnav input{
  width: 70%;
  height: 50rpx;
  border-radius: 30rpx;
  border: 1rpx solid #6495ED;
  position: absolute;
  top: 15rpx;
  font-size: 25rpx;
  background: white;
  position: relative;
  left: 27%;
}
.topnav view{
  background: #6495ED;
  border-radius: 30rpx;
  width: 160rpx;
  height: 55rpx;
  position: absolute;
  right: 2.5%;
  top: 15rpx;
  font-size: 30rpx;
  color: white;
  text-align: center;
  line-height: 50rpx;
  z-index: 1001;
}
.topnav image{
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  top: 23rpx;
  left: 28%;
  z-index: 100;
}
.topnav .adressbox{
  position: absolute;
  width: 25%;
  height: 55rpx;
  background-color: rgb(189, 189, 189);
  left: 1%;
  border: 0;
  opacity: 1;
}
.topnav .adressbox image{
  position: absolute;
  left: 3%;
  top: 12%;
}
.topnav .adressbox view{
  width: 70%;
  height: 100%;
  overflow: scroll;
  position: absolute;
  top: 0;
  left: 25%;
  background: none;
}
.topnav .adressbox text{
  display: block;
  white-space:nowrap;
}
.ROMapBox{
  width: 95%;
  height: 250rpx;
  position: relative;
  left: 2.5%;
  background: white;
  margin: 20rpx 0;
  border-radius: 15rpx;
  border: 1rpx solid rgb(237, 237, 237);
}
.ROMapBox .swiperClass {
  position: relative;
  top: 5%;
  height: 90%;
}
.ROMapBox .slide-image {
  width: 100%;
  height: 100%;
  border-radius: 10px;
  position: relative;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, .8)
}
.ROMapBox image.active {
  transform: none;
  transition: all 0.2s ease-in 0s;
}
.ROMapBox image.quiet {
  transform: scale(0.8333333);
  transition: all 0.2s ease-in 0s;
}
.storesitembox{
  width: 95%;
  height: auto;
  position: relative;
  left: 2.5%;
  margin: 20rpx 0;
  border-radius: 15rpx;
}
.storesitembox .item{
  width: 48%;
  background-color: white;
  position: relative;
  display: inline-block;
  border: 1rpx solid rgb(237, 237, 237);
  padding-bottom: 20rpx;
  border-radius: 5rpx;
}
.storesitembox .left{
  float: left;
}
.storesitembox .right{
  margin-left: 2%;
}
.storesitembox .item .imagebox{
  width: 100%;
  display: block;
  height: 250rpx;
  overflow: hidden;
}
.storesitembox .item .imagebox image{
  max-width: 100%;  
  max-height: 100%;
}
.storesitembox .item .name{
  font-size: 35rpx;
  color: rgb(87, 87, 87);
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 90%;
  margin: 0 auto;
  font-weight: 700;
}
.storesitembox .item .pricebox{
  width: 95%;
  margin: 0 auto;
  height: 60rpx;
}
.storesitembox .item .pricebox .price{
  font-size: 40rpx;
  color: tomato;
}
.storesitembox .item .pricebox .salesnum{
  font-size: 30rpx;
  color: rgb(163, 163, 163);
  display: inline-block;
  margin-left: 20rpx;
}
.storesitembox .item .btype{
  padding: 2rpx;
  height: auto;
  background-color:  tomato;
  font-size: 25rpx;
  padding: 5rpx;
  color: white;
  margin-left: 5%;
  border-radius: 10rpx;
}
.center{
  width: 95%;
  height: 310rpx;
  position: relative;
  left: 2.5%;
  background: white;
  margin: 20rpx 0;
  border-radius: 15rpx;
  border: 1rpx solid rgb(237, 237, 237);
}
.center view{
  width: 100rpx;
  height: 130rpx;
  margin: 10rpx 22rpx;
  display: inline-block;
  position: relative;
  overflow: hidden;
}
.center view image{
  width: 80%;
  height: 60%;
  position: relative;
  left: 10%;
}
.center view text{
  font-size: 25rpx;
  color: rgb(112, 112, 112);
  position: absolute;
  top: 90rpx;
  left: 0;
}
.centershow{
  width: 95%;
  height: 200rpx;
  position: relative;
  left: 2.5%;
  margin: 20rpx 0;
  border-radius: 15rpx;
  overflow: hidden;
  background-color: white;
  border: 1rpx solid rgb(237, 237, 237);
}
.centershow .header{
  width: 95%;
  margin: 0 auto;
  height: 30%;
}
.centershow .header .skilltext{
  font-size: 35rpx;
  font-weight: 700;
}
.centershow .header .timebox{
  width: 250rpx;
  height: 80%;
  background-color: rgb(48, 129, 223);
  border-radius: 30rpx;
  display: inline-block;
  position: relative;
  left: 2%;
  font-size: 30rpx;
  color: white;
  padding: 0 10rpx;
  line-height: 50rpx;
  text-align: center;
}
.centershow .header .more{
  position: absolute;
  right: 20rpx;
  font-size: 30rpx;
  color: #6495ED;
  top: 5rpx;
}
.centershow .contenter{
  width: 95%;
  margin: 0 auto;
  height: 70%;
}
.centershow .contenter .itemer{
  height: 90%;
  width: 200rpx;
  margin-right: 20rpx;
  display: inline-block;
  overflow: hidden;
}
.centershow .contenter .noitemer{
  text-align: center;
  height: 100%;
  line-height: 100rpx;
  color: rgb(128, 128, 128);
  text-align: center;
}
.centershow .contenter .itemer image{
  width: 100%;
  height: 60%;
}
.centershow .contenter .itemer .price{
  font-size: 35rpx;
  color: tomato;
}
.centershow .contenter .itemer .discount{
  font-size: 25rpx;
  margin-left: 5rpx;
  color: rgb(114, 114, 114);
}
.lodingscreen{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1001;
  background: rgba(255, 255, 255, 0.5);
  backdrop-filter: blur(10px);
}