page{
  width: 100%;
  height: 100%;
}
.nothaveorder{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
}
.nothaveorder image{
  width: 500rpx;
  margin: 100rpx auto;
  height: 500rpx;
  display: block;
}
.nothaveorder text{
  display: block;
  text-align: center;
  color: rgb(112, 112, 112);
}
.videobox{
  width: 100rpx;
  height: 50rpx;
  background-color: rgb(199, 199, 199);
  border-radius: 30px;
  z-index: 1001;
  position: fixed;
  top: 20rpx;
  right: 20rpx;
  overflow: hidden;
  border: 1rpx solid rgb(228, 228, 228);
}
.videobox .ctrbtn{
  width: 50rpx;
  height: 50rpx;
  background-color: white;
  border-radius: 50%;
  text-align: center;
  line-height: 50rpx;
  font-size: 25rpx;
  color: rgb(109, 109, 109);
}
.contentitembox{
  position: fixed;
  width: 100%;
  top: 80rpx;
  height: calc(100% - 80rpx);
  position: relative;
}
.contentitembox .item_1{
  width: 100%;
  height: 200rpx;
  background-color: white;
  border-bottom: 1rpx solid rgb(218, 218, 218);
  position: relative;
}
.contentitembox .item_2{
  width: 100%;
  height: 200rpx;
  background-color: white;
  border-bottom: 1rpx solid rgb(218, 218, 218);
  position: relative;
}
.contentitembox .item_1 .name,.contentitembox .item_2 .name{
  position: absolute;
  font-weight: 700;
  font-size: 40rpx;
  top: 10rpx;
  left: 150rpx;
  color: rgb(94, 94, 94);
  display: block;
  width: 60%;
  height: 50rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.contentitembox .item_1 image,.contentitembox .item_2 image{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  height: 130rpx;
  width: 130rpx;
}
.contentitembox .item_1 .delete,.contentitembox .item_2 .delete{
  position: absolute;
  top: 90rpx;
  right: 10rpx;
  width: 80rpx;
  height: 30rpx;
  background-color: rgb(165, 165, 165);
  padding: 10rpx;
  color: white;
  line-height: 30rpx;
  border-radius: 10rpx;
  font-size: 30rpx;
  text-align: center;
}
.contentitembox .item_1  .type,.contentitembox .item_2 .type{
  position: absolute;
  width: auto;
  height: auto;
  font-size: 30rpx;
  background-color: tomato;
  padding: 5rpx;
  border-radius: 10rpx;
  color: white;
  top: 10rpx;
  right: 10rpx;
}
.contentitembox .item_1 .price,.contentitembox .item_2 .price{
  position: absolute;
  width: auto;
  height: auto;
  font-size: 35rpx;
  border-radius: 5rpx;
  color: rgb(134, 26, 26);
  font-weight: 600;
  top: 90rpx;
  left: 150rpx;
}
.contentitembox .item_1 .checkurl,.contentitembox .item_2 .checkurl{
  position: absolute;
  top: 90rpx;
  left: 300rpx;
  height: 40rpx;
  width: 100rpx;
  color: white;
  background-color: tomato;
  font-size: 35rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 10rpx;
}
.contentitembox .item_1 .adress,.contentitembox .item_2 .adress{
  position: absolute;
  top: 130rpx;
  left: 150rpx;
  width: auto;
  height: auto;
  padding: 10rpx;
  font-size: 35rpx;
}