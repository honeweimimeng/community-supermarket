<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hone.serviceorder.dao.AlterOrderMapper">
    <update id="AlterOrderStatus" parameterType="string">
        update orderbaseinfo set breadstatus=1,status=status+1 where oid=${oid};
    </update>
</mapper>